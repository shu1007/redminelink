{
  "name": "redminelink",
  "publisher": "hoge",
  "displayName": "RedmineLink",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.redmineLink"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.redmineLink",
        "title": "RedmineLink"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.redmineLink",
          "title": "RedmineLink"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "redmineLink configuration",
      "properties": {
        "redmineLink.baseUrl": {
          "type": "array",
          "default": [
            {
              "title": "URL1",
              "url": "http://foo/bar/repository/revisions/{RN}/entry/{FP}#L{LN}"
            },
            {
              "title": "URL2",
              "url": "source:\"{FP}@{RN}#L{LN}\""
            }
          ]
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  }
}
